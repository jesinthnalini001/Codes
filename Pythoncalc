from bs4 import BeautifulSoup

def check_nested_table(html):
    try:
        soup = BeautifulSoup(html, 'html.parser')
        # Find all table tags
        table_tags = soup.find_all('table')
        stack = []

        for tag in table_tags:
            if tag.name == 'table':
                stack.append(tag)
            elif tag.name == '/table':
                if not stack:
                    return False
                if tag == stack[-1]:
                    stack.pop()
                else:
                    return False
        
        return len(stack) == 0
    except Exception as e:
        print("Error parsing HTML:", e)
        return False

# Read HTML content from a text file
file_path = "path/to/your/file.html"
with open(file_path, "r") as file:
    html_content = file.read()

if check_nested_table(html_content):
    print("Nested table tags are properly closed.")
else:
    print("Nested table tags are not properly closed.")
